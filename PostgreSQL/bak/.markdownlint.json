{
  "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/main/schema/markdownlint-config-schema.json",
  "default": true,
  "MD001": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 2,
    "start_indented": false
  },
  "MD009": {
    "br_spaces": 2,
    "list_item_empty_lines": false,
    "strict": false
  },
  "MD010": true,
  "MD012": {
    "maximum": 2
  },
  "MD013": false,
  "MD014": false,
  "MD022": {
    "lines_above": 1,
    "lines_below": 1
  },
  "MD024": {
    "siblings_only": true
  },
  "MD025": {
    "level": 1,
    "front_matter_title": ""
  },
  "MD026": {
    "punctuation": ".,;:!。，；：！"
  },
  "MD029": {
    "style": "ordered"
  },
  "MD030": {
    "ul_single": 1,
    "ol_single": 1,
    "ul_multi": 1,
    "ol_multi": 1
  },
  "MD031": {
    "list_items": true
  },
  "MD032": true,
  "MD033": false,
  "MD034": true,
  "MD035": {
    "style": "consistent"
  },
  "MD036": true,
  "MD037": true,
  "MD038": true,
  "MD039": true,
  "MD040": true,
  "MD041": true,
  "MD042": true,
  "MD043": false,
  "MD044": false,
  "MD045": true,
  "MD046": {
    "style": "fenced"
  },
  "MD047": true,
  "MD048": {
    "style": "backtick"
  },
  "MD049": {
    "style": "underscore"
  },
  "MD050": {
    "style": "asterisk"
  },
  "MD051": true,
  "MD052": false,
  "MD053": false
}

